{
    "name": "rsshub",
    "version": "1.0.0",
    "description": "Make RSS Great Again!",
    "keywords": [
        "RSS",
        "feed",
        "atom",
        "json"
    ],
    "homepage": "https://github.com/DIYgod/RSSHub#readme",
    "bugs": {
        "url": "https://github.com/DIYgod/RSSHub/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DIYgod/RSSHub.git"
    },
    "license": "MIT",
    "author": "DIYgod",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "engines": {
        "node": ">=22"
    },
    "scripts": {
        "build": "node scripts/build-package.mjs",
        "prepublishOnly": "pnpm build",
        "build:docs": "tsx scripts/workflow/build-docs.ts",
        "dev": "cross-env NODE_ENV=dev tsx watch --inspect --clear-screen=false lib/index.ts",
        "dev:cache": "cross-env NODE_ENV=production tsx watch --clear-screen=false lib/index.ts",
        "format": "prettier --write \"**/*.{js,json}\"",
        "format:check": "prettier --check \"**/*.{js,json}\"",
        "format:staged": "lint-staged",
        "jest": "cross-env NODE_ENV=test jest --runInBand --forceExit --detectOpenHandles",
        "jest:coverage": "cross-env NODE_ENV=test jest --coverage --reporters=default --reporters=jest-junit --runInBand --forceExit --detectOpenHandles",
        "lint": "eslint --ext .js,.ts,.tsx .",
        "lint:fix": "eslint --fix --ext .js,.ts,.tsx .",
        "lint:staged": "lint-staged",
        "prepare": "husky install",
        "profiling": "NODE_ENV=production node --prof lib/index.js",
        "start": "cross-env NODE_ENV=production node lib/index.js",
        "test": "npm run jest",
        "test:coverage": "npm run jest:coverage"
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,yml}": [
            "prettier --write"
        ],
        "package.json": [
            "sort-package-json"
        ]
    },
